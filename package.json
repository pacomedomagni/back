    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.20",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.4.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
